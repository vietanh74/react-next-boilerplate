{
  "name": "cs-request",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "start": "next start",
    "types-check": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check \"src/**/*+(js|jsx|ts|tsx|scss)\"",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,tsx}\"",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint": "npm run lint:script && npm run lint:prettier && npm run lint:style",
    "lintfix": "prettier --write --list-different \"src/**/*+(js|jsx|ts|tsx|scss)\" && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "^4.21.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "dompurify": "^2.3.8",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.0",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "next-plugin-antd-less": "^1.8.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss}": [
      "npm run lint"
    ]
  },
  "resolutions": {
    "@turf/difference": "6.0.1"
  }
}
